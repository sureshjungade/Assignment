package com.first_project;

class First {

	static void naturalSum(int one, int two, int three, int four,int six) {
		
		int totalscore = (one + (two*2) + (three*3) + (four*4) + (six*6));
		System.out.println(totalscore);
  	}

	public static void main(String[] args){

		naturalSum(1,2,3,4,6);

	}

}

